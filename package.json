{
  "name": "rohan-chandra-website",
  "version": "1.0.0",
  "private": true,
  "description": "Rohan Chandra's personal website",
  "author": "Rohan Chandra",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop --port 8000",
    "start": "gatsby develop  --port 8000",
    "build": "gatsby build",
    "serve": "gatsby serve --port 9000",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yaml",
    "codegen:watch": "graphql-codegen --config codegen.yaml --watch",
    "cy:dev": "cypress open --config baseUrl=http://localhost:8000",
    "cy:prod": "cypress run --config baseUrl=http://localhost:9000",
    "test:dev": "CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:dev",
    "test:ci": "CYPRESS_SUPPORT=y yarn build && start-server-and-test develop http://localhost:8000 serve http://localhost:9000 cy:prod"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "^4.3.0",
    "@fontsource/nunito-sans": "^4.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.4.1",
    "gatsby-plugin-image": "^1.4.1",
    "gatsby-plugin-mdx": "^2.5.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sharp": "^3.4.2",
    "gatsby-plugin-styled-components": "^4.4.0",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "normalize.css": "^8.0.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "refractor": "^4.5.0",
    "remark-slug": "^6.0.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.4.0",
    "cypress": "^7.3.0",
    "gatsby-cypress": "^1.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.12.2",
    "typescript": "^4.2.4"
  }
}